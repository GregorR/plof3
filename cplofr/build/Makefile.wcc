CC=wcc386

CPU=5
CALLING=s
CFLAGS=-$(CPU)$(CALLING)

LINK=wlink
LFLAGS=system dos4g
LIBS=library gc

PSLI_OBJS=psli.obj psl.obj

all: psli.exe

psli.exe: $(PSLI_OBJS)
	$(LINK) $(LFLAGS) $(LIBS) option stack=10485760 file psli file psl

.c.obj:
	$(CC) $(CFLAGS) $<

clean:
	del psli.exe psli.obj psl.obj
