var x = "Bad"

var A = Object : [
    x = "Good"

    B = Object : [
        foo = { x }
    ]
]

// original prototype
Debug.print(A.B.foo())

// instance
var ab = new (A.B)()
Debug.print(ab.foo())

// derived prototype
var a = new A()
var C = a.B : [
    bar = { x }
]
var c = new C()
Debug.print(c.foo())
